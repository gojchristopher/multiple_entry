{
  "version": "0.1.0",
  "license": "MIT",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "type": "module",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=12.20.0"
  },
  "exports":{
    "./accordion":"./dist/components/"
  },
  "scripts": {
    "start": "tsdx watch",
    "build": "microbundle src/*.tsx --jsx React.createElement --jsxFragment React.Fragment -o dist/",
    "test": "tsdx test --passWithNoTests",
    "lint": "tsdx lint",
    "prepare": "microbundle src/*.tsx --jsx React.createElement --jsxFragment React.Fragment -o dist/",
    "size": "size-limit",
    "analyze": "size-limit --why",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "peerDependencies": {
    "react": ">=16"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "name": "@sample_package/test_package",
  "author": "John Christopher Go",
  "module": "dist/test_package.esm.js",
  "size-limit": [
    {
      "path": "dist/test_package.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/test_package.esm.js",
      "limit": "10 KB"
    }
  ],
  "devDependencies": {
    "rollup-plugin-typescript2": "^0.34.1",
    "ttypescript": "^1.5.15"
  }
}
