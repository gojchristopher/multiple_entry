{
  "version": "0.0.48",
  "license": "MIT",
  "main": "dist/test_packages.cjs",
  "typings": "dist/src/index.d.ts",
  "type": "module",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=12.20.0"
  },
  "exports": {
    "./accordion": {
      "node": "./dist/accordion.cjs",
      "import": "./dist/accordion.modern.js"
    },
    "./accordionTypes-types": {
      "node": "./dist/accordionTypes-types.cjs",
      "import": "./dist/accordionTypes-types.modern.js"
    },
    "./button": {
      "node": "./dist/button.cjs",
      "import": "./dist/button.modern.js"
    }
  },
  "typesVersions": {
    "*": {
      "accordion": [
        "dist/src/accordion.d.ts"
      ],
      "accordionTypes-types": [
        "dist/src/accordionTypes-types.d.ts"
      ],
      "button": [
        "dist/src/button.d.ts"
      ]
    }
  },
  "scripts": {
    "start": "tsdx watch",
    "build": "rm -rf dist && microbundle -i src/*.tsx -f cjs,modern,esm -o dist/",
    "test": "tsdx test --passWithNoTests",
    "lint": "tsdx lint",
    "size": "size-limit",
    "analyze": "size-limit --why",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "peerDependencies": {
    "react": ">=16"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "name": "@opensourcerist/test_package",
  "author": "John Christopher Go",
  "module": "dist/test_package.esm.js",
  "size-limit": [
    {
      "path": "dist/test_package.cjs",
      "limit": "10 KB"
    },
    {
      "path": "dist/test_package.esm.js",
      "limit": "10 KB"
    }
  ],
  "publishConfig": {
    "access": "public"
  },
  "gitHead": "8edf8dc88d2271ddcdc3a9f0a14d9274596215e8"
}
