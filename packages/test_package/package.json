{
  "version": "0.0.42",
  "license": "MIT",
  "main": "dist/test_packages.cjs",
  "typings": "dist/src/index.d.ts",
  "type": "module",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=12.20.0"
  },
  "browser": {
    "./dist/test_package.cjs": "./dist/test_package.cjs.br?brotli=true",
    "./dist/accordion.cjs": "./dist/accordion.cjs.br?brotli=true",
    "./dist/accordionTypes-types": "./dist/accordionTypes-types.cjs.br?brotli=true"
  },
  "exports": {
    "./accordion": {
      "node": "./dist/accordion.cjs",
      "import": "./dist/accordion.modern.js"
    },
    "./accordionTypes-types": {
      "node": "./dist/accordionTypes-types.cjs",
      "import": "./dist/accordionTypes-types.modern.js"
    },
    "./button": {
      "node": "./dist/button.cjs",
      "import": "./dist/button.modern.js"
    }
  },
  "scripts": {
    "start": "tsdx watch",
    "build": "rm -rf dist && microbundle -i src/*.tsx --tsconfig ./tsconfig.json -f cjs,modern",
    "test": "tsdx test --passWithNoTests",
    "lint": "tsdx lint",
    "size": "size-limit",
    "analyze": "size-limit --why",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "peerDependencies": {
    "react": ">=16"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "name": "@gojchristopher/test_package",
  "author": "John Christopher Go",
  "module": "dist/test_package.modern.js",
  "publishConfig": {
    "access": "public"
  },
