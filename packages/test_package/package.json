{
  "version": "0.0.4",
  "license": "MIT",
  "main": "dist/test_packages.cjs",
  "typings": "dist/index.d.ts",
  "type": "module",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=12.20.0"
  },
  "exports": {
    "./accordion": {
      "node": "./dist/accordion.cjs",
      "import": "./dist/accordion.modern.js"
    },
    "./button": "./dist/button.cjs"
  },
  "typesVersions": {
    "*": {
      "accordion": [
        "dist/accordion.d.ts"
      ],
      "button": [
        "dist/button.d.ts"
      ]
    }
  },
  "scripts": {
    "start": "tsdx watch",
    "build": "rm -rf dist && microbundle build src/*.tsx --jsx React.createElement --jsxFragment React.Fragment -o dist/ -f cjs,modern",
    "test": "tsdx test --passWithNoTests",
    "lint": "tsdx lint",
    "size": "size-limit",
    "analyze": "size-limit --why",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "peerDependencies": {
    "react": ">=16"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "name": "@go-test-packages/test_package",
  "author": "John Christopher Go",
  "module": "dist/test_package.esm.js",
  "size-limit": [
    {
      "path": "dist/test_package.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/test_package.esm.js",
      "limit": "10 KB"
    }
  ],
  "devDependencies": {
    "rollup-plugin-typescript2": "^0.34.1",
    "ttypescript": "^1.5.15"
  },
  "publishConfig": {
    "access": "public"
  },
  "gitHead": "57120654b82e243e91314a3d7dd2399182d4e725"
}
