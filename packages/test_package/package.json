{
  "version": "0.0.32",
  "license": "MIT",
  "main": "dist/test_packages.js",
  "typings": "dist/src/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=12.20.0"
  },
  "exports": {
    "./accordion": {
      "require": "./dist/accordion.js",
      "default": "./dist/accordion.modern.js"
    },
    "./accordionTypes-types": {
      "require": "./dist/accordionTypes-types.js",
      "default": "./dist/accordionTypes-types.modern.js"
    },
    "./button": {
      "require": "./dist/button.js",
      "default": "./dist/button.modern.js"
    }
  },
  "typesVersions": {
    "*": {
      "accordion": [
        "dist/src/accordion.d.ts"
      ],
      "button": [
        "dist/src/button.d.ts"
      ],
      "accordionTypes-types": [
        "dist/src/accordionTypes-types.d.ts"
      ]
    }
  },
  "sideEffects": false,
  "scripts": {
    "start": "tsdx watch",
    "build": "rm -rf dist && microbundle build src/*.tsx -o dist/ -f cjs,modern,  --compress",
    "prepare": "rm -rf dist && microbundle build src/*.tsx -o dist/ -f cjs,modern --compress",
    "test": "tsdx test --passWithNoTests",
    "lint": "tsdx lint",
    "size": "size-limit",
    "analyze": "size-limit --why",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "peerDependencies": {
    "react": ">=16"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "name": "@gojchristopher/test_package",
  "author": "John Christopher Go",
  "module": "dist/test_package.esm.js",
  "size-limit": [
    {
      "path": "dist/test_package.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/test_package.esm.js",
      "limit": "10 KB"
    }
  ],
  "publishConfig": {
    "access": "public"
  },
  "gitHead": "e4ff8224355723c1e03c9e46af16e263cec241c8"
}
