{
  "version": "0.1.7",
  "license": "MIT",
  "main": "dist/test_accordion.js",
  "umd:main": "dist/test_accordion.umd.js",
  "module": "dist/test_accordion.modern.js",
  "typings": "dist/src/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=10"
  },
  "exports": {
    "./accordionTypes": "./dist/accordionType-types.modern.js"
  },
  "typesVersions": {
    "*": {
      "accordionTypes": [
        "dist/src/accordionType-types.d.ts"
      ]
    }
  },
  "scripts": {
    "start": "tsdx watch",
    "build": "rm -rf dist && microbundle -i src/*.tsx -f cjs,umd,modern -o dist/",
    "test": "tsdx test --passWithNoTests",
    "lint": "tsdx lint",
    "size": "size-limit",
    "analyze": "size-limit --why",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "peerDependencies": {
    "react": ">=16"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "name": "@gojchristopher/test_accordion",
  "author": "John Christopher Go",
  "size-limit": [
    {
      "path": "dist/test_accordion.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/test_accordion.modern.js",
      "limit": "10 KB"
    }
  ],
  "publishConfig": {
    "access": "public"
  }
}
