{
  "version": "0.1.2",
  "license": "MIT",
  "main": "dist/accordion.cjs",
  "typings": "dist/index.d.ts",
  "type": "module",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "start": "tsdx watch",
    "build": "rm -rf dist && microbundle -i src/*.tsx --tsconfig ./tsconfig.json -f cjs,modern",
    "test": "tsdx test --passWithNoTests",
    "lint": "tsdx lint",
    "prepare": "tsdx build",
    "size": "size-limit",
    "analyze": "size-limit --why",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "peerDependencies": {
    "react": ">=16"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "name": "@gojchristopher/test_accordion",
  "author": "gojchristopher.github.io",
  "module": "dist/test_accordion.modern.js",
  "size-limit": [
    {
      "path": "dist/test_accordion.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/test_accordion.modern.js",
      "limit": "10 KB"
    }
  ],
  "publishConfig": {
    "access": "public"
  }
}
