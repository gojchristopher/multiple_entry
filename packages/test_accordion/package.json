{
  "version": "0.1.17",
  "license": "MIT",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "typings": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=12.20.0"
  },
  "tsup": {
    "clean": true,
    "target": "es2019",
    "format": [
      "cjs",
      "esm"
    ]
  },
  "sideEffect": false,
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.ts"
    },
    "./accordionTypes": {
      "require": "./dist/accordionType-types.js",
      "import": "./dist/accordionType-types.mjs",
      "types": "./dist/accordionType-types.d.ts"
    }
  },
  "typesVersions": {
    "*": {
      "accordionTypes": [
        "dist/accordionType-types.d.ts"
      ]
    }
  },
  "scripts": {
    "start": "tsdx watch",
    "build": "rm -rf dist && tsup src --dts",
    "test": "tsdx test --passWithNoTests",
    "lint": "tsdx lint",
    "size": "size-limit",
    "analyze": "size-limit --why",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "peerDependencies": {
    "react": ">=16"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "name": "@opensourcerist/test_accordion",
  "author": "John Christopher Go",
  "size-limit": [
    {
      "path": "dist/test_accordion.cjs",
      "limit": "10 KB"
    },
    {
      "path": "dist/test_accordion.esm.js",
      "limit": "10 KB"
    }
  ],
  "publishConfig": {
    "access": "public"
  }
}
